{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/find-bug", "dest": "api/find-bug.py" },
    { "src": "/sample-cases", "dest": "api/find-bug.py" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ]
}
